<?xml version="1.0" encoding="windows-1252"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:j.0="http://www.exemplo.com/onto1#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Class rdf:about="http://www.exemplo.com/onto1#Pessoa"/>
  <owl:Class rdf:about="http://www.exemplo.com/onto1#Masculino">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.exemplo.com/onto1#temConjuge"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://www.exemplo.com/onto1#Feminino"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.exemplo.com/onto1#temConjuge"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.exemplo.com/onto1#Feminino"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.exemplo.com/onto1#Pessoa"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.exemplo.com/onto1#Feminino">
    <owl:disjointWith rdf:resource="http://www.exemplo.com/onto1#Masculino"/>
    <rdfs:subClassOf rdf:resource="http://www.exemplo.com/onto1#Pessoa"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.exemplo.com/onto1#PessoaCasada">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://www.exemplo.com/onto1#Pessoa"/>
          <owl:Restriction>
            <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:minCardinality>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="http://www.exemplo.com/onto1#temConjuge"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.exemplo.com/onto1#temIrmao">
    <rdfs:range rdf:resource="http://www.exemplo.com/onto1#Pessoa"/>
    <rdfs:domain rdf:resource="http://www.exemplo.com/onto1#Pessoa"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.exemplo.com/onto1#temConjuge">
    <rdfs:range rdf:resource="http://www.exemplo.com/onto1#Pessoa"/>
    <rdfs:domain rdf:resource="http://www.exemplo.com/onto1#Pessoa"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.exemplo.com/onto1#temIdade">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="http://www.exemplo.com/onto1#Pessoa"/>
  </owl:DatatypeProperty>
  <j.0:Feminino rdf:about="http://www.exemplo.com/onto1#Maria">
    <j.0:temConjuge>
      <j.0:Masculino rdf:about="http://www.exemplo.com/onto1#José">
        <j.0:temConjuge rdf:resource="http://www.exemplo.com/onto1#Maria"/>
      </j.0:Masculino>
    </j.0:temConjuge>
    <j.0:temIrmao>
      <j.0:Masculino rdf:about="http://www.exemplo.com/onto1#João">
        <j.0:temIrmao rdf:resource="http://www.exemplo.com/onto1#Maria"/>
        <j.0:temIdade rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >20</j.0:temIdade>
      </j.0:Masculino>
    </j.0:temIrmao>
  </j.0:Feminino>
</rdf:RDF>
